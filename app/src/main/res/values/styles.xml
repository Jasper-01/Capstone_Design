<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="generalBtn1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/color_4</item>
        <item name="android:textColor">@color/color_2</item>
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="generalBtn2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/color_5</item>
        <item name="android:textColor">@color/color_2</item>
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="plannerPageBoxes">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/color_5</item>
        <item name="android:textColor">@color/color_2</item>
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="plannerPageBtnBox">
        <item name="android:layout_width">50dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/button_table</item>
    </style>

    <style name="searchIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:maxWidth">25dip</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="tableRow">
        <item name="android:layout_width">250dip</item>
        <item name="android:layout_height">50dip</item>
    </style>

    <style name="generalText1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_3</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/alata</item>
    </style>

    <style name="generalText2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_4</item>
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/alata</item>
    </style>

    <style name="inputPopupBtn">
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textColor">@color/color_3</item>
        <item name="android:backgroundTint">@color/color_1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="keywordPopupText1">
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textColor">@color/color_2</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="keywordPopupText2">
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textColor">@color/color_4</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="keywordPopupBox">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="loginPageLoginBtn">
        <item name="android:layout_width">250dip</item>
        <item name="android:layout_height">100dip</item>
        <item name="android:fontFamily">@font/alata</item>
        <item name="android:textSize">30sp</item>
        <item name="android:backgroundTint">@color/color_4</item>
        <item name="android:textColor">@color/color_2</item>
    </style>

    <style name="logo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">100dip</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="Circular">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>
    </style>

</resources>